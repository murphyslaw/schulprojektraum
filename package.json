{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "docker:build:dev": "docker compose --file compose.dev.yml build",
    "docker:build:prod": "docker compose --file compose.prod.yml build",
    "docker:dev": "docker compose --file compose.dev.yml up --renew-anon-volumes",
    "docker:prod": "docker compose --file compose.prod.yml up"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nuxt": "^3.8.1",
    "nuxt-primevue": "^0.2.0",
    "prettier": "^3.1.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "primeicons": "^6.0.1"
  }
}
